<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/btnPublic"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="visit public method" />

    <Button
        android:id="@+id/btnPackage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="visit package method" />


    <Button
        android:id="@+id/btnProtected"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="visit protected method" />


    <Button
        android:id="@+id/btnPrivate"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="visit private method" />

    <fragment
        android:id="@+id/fragment"
        android:name="com.yn.asmbutterknife.TestFragment"
        android:layout_width="match_parent"
        android:layout_height="80dp" />


</LinearLayout>
